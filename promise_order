new Promise(resolve => {
  console.log(1);
  console.log(2)
  resolve();
  console.log(3)

  new Promise(resolve => {
    console.log(4)
    resolve();
  }).then(() => console.log(7)).then(()=>console.log(10));

  new Promise(reject =>{
    console.log(5);
    reject();
  }).then(()=>console.log(8)).then(()=>console.log(11))

}).then(num => {
  console.log(9)
}).then(()=>console.log(12)).then(()=>console.log(13))

console.log(6)